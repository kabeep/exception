{
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": [
            "jsdoc"
        ]
    },
    "source": {
        "include": [
            "dist",
            "package.json",
            "README.md"
        ],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "opts": {
        "template": "node_modules/docdash/",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
